{
    "name": "T.M.M. Quotes",
    "description": "Replaces any text on a webpage that is surrounded by quotes with a random T.M.M. lyric!",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "quote_replacer.js"
            ],
            "all_frames": true,
            "run_at": "document_idle"
        }
    ]
}